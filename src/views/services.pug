block config

doctype html
html(lang="ru", class="no-js")
    head
        block head
            include _includes/head
    body.body
        include _includes/mixins/svgsprite
        
        block header
            include _includes/blocks/header

        .content#services
            block content
                block leader
                    include _includes/blocks/services/servicesLeader

                block servicesCards
                    include _includes/blocks/services/servicesCards

                block questions
                    include _includes/blocks/product/questions

                block getProject
                    include _includes/blocks/product/getProject
                
                block benefits2
                    include _includes/blocks/product/benefits2
               
               
               
        
        block footer
            include _includes/blocks/footer
        
        // include popups here
        block modal
            include _includes/blocks/modal

        block styles
          link(rel='stylesheet', href='assets/css/services.css?v=' + PACKAGE.version)
        block scripts-config
            script.
                /* изменяемые настройки скриптов, указаны значения по умолчанию */
                var appConfig = {
                    /*
                    scrollToOffset: 150, // оффсет при скролле до элемента
                    scrollToSpeed: 500, // скорость скролла 
                    */
                }
        block scripts
            script(src='assets/js/vendors~main.bundle.js')
